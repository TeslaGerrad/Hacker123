{
  "name": "hacker123",
  "version": "0.1.2",
  "description": "A software Game for Ethical Hackers and programmers",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:windows": "electron-builder --win --dir",
    "dist:windows": "electron-builder --win",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "ElishaGerrad",
  "license": "MIT",
  "devDependencies": {
    "electron": "^24.8.8",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "appId": "com.biztec.app",
    "productName": "Hacker123",
    "win": {
      "icon": "hacker123.ico"
    },
    "target": "NSIS",
    "nsis": {
      "installerIcon": "hacker123.ico",
      "uninstallerIcon": "hacker123.ico",
      "license": "LICENSE",
      "runAfterFinish": true,
      "oneClick": false
    }
  }
}
